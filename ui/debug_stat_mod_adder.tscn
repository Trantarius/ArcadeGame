[sub_resource type="GDScript" id="GDScript_js357"]
script/source = "extends PanelContainer

@export var mod_name_node:LineEdit
@export var stage_node:OptionButton
@export var strength_node:SpinBox
@export var stacks_node:SpinBox
@export var stat_name_node:LineEdit
@export var target_node:OptionButton

func _on_add_button_pressed() -> void:
	var player:Player = get_tree().get_first_node_in_group(&'Players')
	if(is_instance_valid(player)):
		var mod:StatMod = StatMod.new()
		mod.mod_name = mod_name_node.text
		mod.stage = stage_node.get_selected_id()
		mod.strength = strength_node.value
		mod.stacks = stacks_node.value
		mod.stat_name = stat_name_node.text
		mod.target = target_node.get_selected_id()
		player.add_child(mod)
	hide()


func _on_cancel_button_pressed() -> void:
	hide()
"

